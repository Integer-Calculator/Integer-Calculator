<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shweta.integercalculator.MainActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="527dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="11dp">


        <Button
            android:id="@+id/button18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="175dp"
            android:text="1" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/button18"
            android:layout_alignBottom="@+id/button18"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp"
            android:layout_toEndOf="@+id/button18"
            android:layout_toRightOf="@+id/button18"

            android:text="2" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button19"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_toEndOf="@+id/button19"
            android:layout_toRightOf="@+id/button19"
            android:text="3" />

        <Button
            android:id="@+id/button21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button20"
            android:layout_toEndOf="@+id/button20"
            android:layout_toRightOf="@+id/button20"
            android:text="4" />

        <Button
            android:id="@+id/button22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/button19"
            android:layout_marginTop="15dp"
            android:text="+" />

        <Button
            android:id="@+id/button23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/button22"
            android:layout_toLeftOf="@+id/button20"
            android:layout_toStartOf="@+id/button20"
            android:text="-" />

        <Button
            android:id="@+id/button24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button20"
            android:layout_alignStart="@+id/button20"
            android:layout_alignTop="@+id/button23"
            android:text="*" />

        <Button
            android:id="@+id/button25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button24"
            android:layout_toEndOf="@+id/button24"
            android:layout_toRightOf="@+id/button24"
            android:text="/" />

        <Button
            android:id="@+id/button26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/button22"
            android:layout_marginTop="20dp"
            android:text="(" />

        <Button
            android:id="@+id/button27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button23"
            android:layout_alignStart="@+id/button23"
            android:layout_alignTop="@+id/button26"
            android:text=")" />

        <Button
            android:id="@+id/button28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button27"
            android:layout_toLeftOf="@+id/button25"
            android:layout_toStartOf="@+id/button25"
            android:text=")" />

        <Button
            android:id="@+id/button29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button28"
            android:layout_toEndOf="@+id/button28"
            android:layout_toRightOf="@+id/button28"
            android:text=")" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="18dp"
            android:layout_marginStart="18dp"
            android:layout_marginTop="37dp"
            android:text="Time"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/max"
            android:layout_alignBottom="@+id/max"
            android:layout_alignLeft="@+id/button20"
            android:layout_alignStart="@+id/button20"
            android:layout_marginLeft="18dp"
            android:layout_marginStart="18dp"
            android:text="Attempt" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView5"
            android:layout_marginTop="40dp"
            android:layout_toLeftOf="@+id/button19"
            android:layout_toStartOf="@+id/button19"
            android:text="Succeeded" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView6"
            android:layout_alignRight="@+id/textView6"
            android:layout_alignTop="@+id/textView7"
            android:text="Skipped" />

        <TextView
            android:id="@+id/max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView7"
            android:layout_marginLeft="22dp"
            android:layout_marginStart="22dp"
            android:layout_toEndOf="@+id/textView5"
            android:layout_toRightOf="@+id/textView5"
            android:maxWidth="100dp"
            android:minWidth="100dp"
            android:background="#fff"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView7"
            android:layout_alignEnd="@+id/max"
            android:layout_alignRight="@+id/max"
            android:maxWidth="50dp"
            android:minWidth="50dp"
            android:background="#fff"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView6"
            android:layout_alignBottom="@+id/textView6"
            android:layout_toEndOf="@+id/button20"
            android:layout_toRightOf="@+id/button20"
            android:background="#fff"
            android:maxWidth="50dp"
            android:minWidth="50dp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignLeft="@+id/textView11"
            android:layout_alignStart="@+id/textView11"
            android:background="#fff"
            android:maxWidth="50dp"
            android:minWidth="50dp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"

            android:layout_centerHorizontal="true"
            android:layout_marginTop="49dp"
            android:background="#fff"
            android:maxWidth="1000dp"
            android:minWidth="1000dp"
            android:maxHeight="130dp"
            android:minHeight="130dp"
            android:visibility="visible" />


    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
